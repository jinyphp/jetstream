{
    "name": "jiny/jetstream",
    "description": "Tailwind scaffolding for the Laravel framework.",
    "keywords": ["laravel", "tailwind", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/jetstream/issues",
        "source": "https://github.com/laravel/jetstream"
    },
    "authors": [
        {
            "name": "HojinLee",
            "email": "infohojin@gmail.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "ext-json": "*",
        "illuminate/console": "^10.17",
        "illuminate/support": "^10.17",
        "jiny/fortify": "^1.19",
        "mobiledetect/mobiledetectlib": "^4.8"
    },
    "require-dev": {
        "inertiajs/inertia-laravel": "^0.6.5",
        "laravel/sanctum": "^3.0",
        "livewire/livewire": "^3.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.11",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Jetstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Jetstream\\Tests\\": "tests/",
            "App\\": "stubs/app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Jiny\\Jetstream\\JetstreamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
